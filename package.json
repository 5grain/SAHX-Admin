{
  "name": "SAHX-Admin",
  "description": "SAHX 后台管理系统",
  "version": "1.0.0",
  "scripts": {
    "start": "node www/development.js",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir app/ --retain-lines",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch": "npm run watch-compile",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "wp": "webpack -w"
  },
  "dependencies": {
    "thinkjs": "2.1.x",
    "request": "^2.7.0",
    "babel-runtime": "6.x.x",
    "vue": "^1.0.15",
    "vue-validator": "^2.1.5",
    "vuex": "^0.6.3"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-core": "6.x.x",
    "babel-loader": "^6.2.1",
    "babel-runtime": "^6.3.19",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.0",
    "vue-html-loader": "^1.1.0",
    "vue-loader": "^7.0.3",
    "vue-router": "^0.7.11",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
